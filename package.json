  "start": "npm-run-all --parallel watcher webpack",
  "build": "webpack --config ./bundler/webpack.prod.js",
  "prettier": "prettier --write",
  "lint": "eslint --debug 'src/**/*.{ts,tsx}'",
  "lint:write": "eslint --fix",
  "watcher": "node scripts/watcher/",
  "bundle-report": "webpack --profile --json --config ./bundler/webpack.prod.js > stats.json && webpack-bundle-analyzer stats.json -m static",
  "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'docs: update changelog.md' && npm version patch && git push origin HEAD && git push origin --tags",
  "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'docs: update changelog.md' && npm version minor && git push origin HEAD && git push origin --tags",
  "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'docs: update changelog.md' && npm version major && git push origin HEAD && git push origin --tags"
 },
